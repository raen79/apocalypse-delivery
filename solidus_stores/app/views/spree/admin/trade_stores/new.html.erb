<% admin_breadcrumb(link_to t('.title'), spree.admin_trade_stores_path) %>
<% admin_breadcrumb(t('.new_store')) %>

<div data-hook="admin_user_new_form_header">
  <%= render partial: 'spree/shared/error_messages', locals: { target: @store } %>
</div>

<div data-hook="admin_store_new_form">
  <%= form_for [:admin, @store], url: admin_trade_stores_url, method: :post do |f| %>
    <%= render partial: 'form', locals: { f: f } %>

    <div data-hook="admin_user_new_form_buttons">
      <%= render partial: 'spree/admin/shared/new_resource_links' %>
    </div>
  <% end %>
</div>